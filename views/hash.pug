          //input.btn.btn-primary(type='button' value='Stamp Upload' onclick="location.href='/stampid' ")
          input.btn.btn-primary(type='button' value= 'Stamp Upload' onclick = 'message()')
          script(type='text/javascript').
            function message(){
                var message = "블록체인에 등록하시겠습니까?";
                var result = confirm(message);
                  if(result==true){
                    alert(" 블록체인에 등기중입니다. 등기완료까지 시간이 소요될 수 있습니다. ");
                    location.href='/stampid';
                  } 
                  else{
                    alert("취소되었습니다");
                    location.href='/mypage';
                  }
            }
