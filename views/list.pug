          artcle
            .container
            br
            .container      
            table.table.table-hover(id='mytable')
                          thead
                            tr
                              th 번호
                              th 제목
                              th 날짜
                          tbody
                          -for (var i = 0; i < result.length; i++){
                            tr(onclick=fp+result[i]._id+"'")
                              td= i+1
                              td= result[i].filename
                              td= result[i].upload_at   
                              -}
      

                  
          a.btn.btn-warning.pull-right(href='/new') 문서등록
    



    
    

      
